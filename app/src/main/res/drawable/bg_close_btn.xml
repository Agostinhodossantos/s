<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:color="#94000000"
                android:width="4dp"/>
            <corners android:radius="60dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <stroke android:color="#94000000"

                android:width="2dp"/>
            <corners android:radius="60dp"/>
        </shape>
    </item>
</selector>